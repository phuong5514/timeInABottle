<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Style x:Key="Cell" TargetType="Border">
        <Setter Property="BorderBrush" Value="Black" />
        <Setter Property="BorderThickness" Value="1" />
    </Style>

    <Style x:Key="CellContent" TargetType="Grid">
        <Setter Property="Padding" Value="{StaticResource SmallPadding}"/>
        <Setter Property="CornerRadius" Value="4"/>
        <Setter Property="Margin" Value="{StaticResource CellContentMargin}"/>

    </Style>

    <!--
    <Style x:Key="CellItemBorder" TargetType="Border">
